{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.php",
            "options": {
                "phpVersion": "8.3",
                "trailingCommaPHP": true
            }
        }
    ]
}
